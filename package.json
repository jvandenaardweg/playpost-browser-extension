{
  "name": "playpost-chrome-extension",
  "version": "1.0.0",
  "description": "Chrome Extension for Playpost",
  "scripts": {
    "start": "npm run dev && npm run watch",
    "build": "webpack --config webpack.prod.js --production",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "lint": "tslint 'src/**/*.{ts, tsx}'",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "author": "Jordy van den Aardweg",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.60",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.11",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-zip-plugin": "^0.1.6"
